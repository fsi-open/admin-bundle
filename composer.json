{
    "name": "fsi/admin-bundle",
    "type": "symfony-bundle",
    "description": "FSi Admin Bundle. Admin generator for Symfony.",
    "keywords": ["admin", "panel", "symfony2", "symfony3", "bundle", "fsi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        },
        {
            "name": "Åukasz Cybula",
            "email": "lukasz@fsi.pl"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle" : "^2.4|^3.0",
        "symfony/form" : "^2.3|^3.0",
        "symfony/intl" : "^2.6|^3.0",
        "symfony/options-resolver": "^2.6|^3.0",
        "symfony/expression-language": "^2.6|^3.0",
        "symfony/dependency-injection": "^2.6.2|^3.0",
        "symfony/validator": "^2.3|^3.0",
        "symfony/proxy-manager-bridge": "^2.3|^3.0",
        "symfony/doctrine-bridge": "^2.3|^3.0",
        "symfony/twig-bridge": "^2.6|^3.0",
        "symfony/translation": "^2.4|^3.0",
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/twig-bundle": "^2.3|^3.0",
        "fsi/datagrid-bundle" : "^1.2",
        "fsi/datasource-bundle": "^1.3@dev",
        "fsi/datasource": "^1.3@dev",
        "knplabs/knp-menu-bundle": "^2.0",
        "doctrine/annotations": "^1.0",
        "doctrine/orm": "^2.4.5"
    },
    "require-dev": {
        "symfony/class-loader": "^2.3|^3.0",
        "symfony/monolog-bundle": "^2.3|^3.0",
        "ext-pdo_sqlite": "*",
        "fsi/resource-repository-bundle": "^1.1.5|^2.0@dev",
        "fsi/doctrine-extensions-bundle": "^1.1.4",
        "doctrine/doctrine-bundle": "^1.4",
        "phpspec/phpspec": "^2.2",
        "behat/behat": "^3.0.10",
        "behat/symfony2-extension": "^2.0",
        "behat/mink-extension": "^2.0",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.2",
        "sensiolabs/behat-page-object-extension": "^2.0",
        "bossa/phpspec2-expect": "^1.0",
        "fzaninotto/faker": "^1.2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.1": "1.1-dev"
        }
    }
}
