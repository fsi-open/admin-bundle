{
    "name": "fsi/admin-bundle",
    "type": "symfony-bundle",
    "description": "FSi Admin Bundle. Admin generator for Symfony.",
    "keywords": ["admin", "panel", "symfony2", "symfony3", "symfony4", "bundle", "fsi", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        },
        {
            "name": "≈Åukasz Cybula",
            "email": "lukasz@fsi.pl"
        },
        {
            "name": "Piotr Szymaszek",
            "email": "piotr.szymaszek@fsi.pl"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.0.3",
        "doctrine/persistence": "^2.3",
        "doctrine/orm": "^2.8",
        "fsi/data": "^1.0@dev",
        "fsi/translatable": "^1.0@dev",
        "knplabs/knp-menu": "^3.1.3",
        "knplabs/knp-menu-bundle": "^3.1",
        "psr/event-dispatcher": "^1.0",
        "sensio/framework-extra-bundle": "^5.5.7|^6.0",
        "symfony/asset": "^4.4|^5.4",
        "symfony/config": "^4.4.12|^5.4",
        "symfony/dependency-injection": "^4.4.12|^5.4",
        "symfony/doctrine-bridge": "^4.4|^5.4",
        "symfony/form" : "^4.4|^5.4",
        "symfony/framework-bundle" : "^4.4|^5.4",
        "symfony/http-foundation": "^4.4|^5.4",
        "symfony/intl" : "^4.4|^5.4",
        "symfony/options-resolver": "^4.4|^5.4",
        "symfony/proxy-manager-bridge": "^4.4|^5.4",
        "symfony/security-csrf": "^4.4|^5.4",
        "symfony/translation": "^4.4.12|^5.4",
        "symfony/twig-bridge": "^4.4|^5.4",
        "symfony/twig-bundle": "^4.4|^5.4",
        "symfony/validator": "^4.4|^5.4",
        "twig/twig": "^2.13|^3.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "behat/behat": "^3.10",
        "behat/mink-selenium2-driver": "^1.3",
        "caciobanu/behat-deprecation-extension": "^2.1",
        "fakerphp/faker": "^1.7",
        "friends-of-behat/mink": "^1.10",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-extension": "^2.6",
        "friends-of-behat/page-object-extension": "^0.3.2",
        "friends-of-behat/symfony-extension": "^2.3",
        "friends-of-phpspec/phpspec-expect": "^4.0",
        "fsi/files": "^1.0",
        "fsi/resource-repository-bundle": "^2.0",
        "gedmo/doctrine-extensions": "^2.4|^3.0",
        "nyholm/psr7": "^1.5",
        "oneup/flysystem-bundle": "^3.7",
        "phpspec/phpspec": "^6.0|^7.0",
        "phpspec/prophecy": "^1.8",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-beberlei-assert": "^1.0",
        "rize/uri-template": "^0.3.1",
        "sebastian/comparator": "^3.0",
        "sebastian/exporter": "^3.1",
        "squizlabs/php_codesniffer": "^3.6.0",
        "symfony/debug": "^4.4|^5.4",
        "symfony/dom-crawler": "^4.4.12|^5.4",
        "symfony/event-dispatcher": "^4.4.27|^5.4",
        "symfony/http-client": "^4.4|^5.4",
        "symfony/monolog-bridge": "^4.4|^5.4",
        "symfony/monolog-bundle": "^3.7",
        "symfony/var-dumper": "^4.4|^5.4",
        "symfony/yaml": "^4.4|^5.4"
    },
    "conflict": {
        "fsi/admin-positionable-bundle": "*",
        "fsi/admin-tree-bundle": "*"
    },
    "config": {
        "bin-dir": "vendor/bin",
        "sort-packages": true
    },
    "scripts": {
        "sniffer": "vendor/bin/phpcs --colors",
        "stan": "vendor/bin/phpstan --ansi analyse -c phpstan.neon",
        "spec": "vendor/bin/phpspec --ansi run --format=pretty",
        "behat": "vendor/bin/behat --colors --no-snippets"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\Bundle\\AdminBundle\\Behat\\": "Behat",
            "FSi\\Bundle\\AdminBundle\\spec\\fixtures\\": "spec/fixtures",
            "FSi\\": "features/fixtures/project/src",
            "spec\\FSi\\Bundle\\AdminBundle\\": "spec/FSi/Bundle/AdminBundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev",
            "3.1": "3.1-dev",
            "3.0": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.1": "1.1-dev"
        }
    }
}
