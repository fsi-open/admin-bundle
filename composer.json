{
    "name": "fsi/admin-bundle",
    "type": "symfony-bundle",
    "description": "FSi Admin Bundle. Admin generator for Symfony.",
    "keywords": ["admin", "panel", "symfony2", "symfony3", "symfony4", "bundle", "fsi", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        },
        {
            "name": "≈Åukasz Cybula",
            "email": "lukasz@fsi.pl"
        },
        {
            "name": "Piotr Szymaszek",
            "email": "piotr.szymaszek@fsi.pl"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.0.3",
        "doctrine/persistence": "^2.0",
        "doctrine/orm": "^2.5",
        "sensio/framework-extra-bundle": "^5.5.7|^6.0",
        "symfony/asset": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/doctrine-bridge": "^4.4|^5.0",
        "symfony/expression-language": "^4.4|^5.0",
        "symfony/form" : "^4.4|^5.0",
        "symfony/framework-bundle" : "^4.4|^5.0",
        "symfony/http-foundation": "^4.4|^5.0",
        "symfony/intl" : "^4.4|^5.0",
        "symfony/options-resolver": "^4.4|^5.0",
        "symfony/proxy-manager-bridge": "^4.4|^5.0",
        "symfony/translation": "^4.4|^5.0",
        "symfony/twig-bridge": "^4.4|^5.0",
        "symfony/twig-bundle": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0",
        "fsi/datagrid-bundle" : "^2.1.2@dev",
        "fsi/datasource-bundle": "^2.1.3@dev",
        "knplabs/knp-menu-bundle": "^2.2.1|^3.0",
        "ocramius/proxy-manager": "^2.1",
        "twig/twig": "^2.11"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "behat/behat": "^3.3",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.0",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "caciobanu/behat-deprecation-extension": "^2.0",
        "friends-of-phpspec/phpspec-expect": "^3.0|^4.0",
        "fsi/datagrid" : "^2.0",
        "fsi/datasource": "^2.0",
        "fsi/doctrine-extensions-bundle": "^2.1.3",
        "fsi/resource-repository-bundle": "^2.0",
        "fakerphp/faker": "^1.7",
        "phpspec/phpspec": "^5.0|^6.0",
        "phpspec/prophecy": "^1.8",
        "phpstan/phpstan": "^0.12.66",
        "rize/uri-template": "^0.3.1",
        "sebastian/comparator": "^3.0",
        "sebastian/exporter": "^3.1",
        "sensiolabs/behat-page-object-extension": "^2.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/debug": "^4.4|^5.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/monolog-bridge": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0",
        "symfony/var-dumper": "^4.4|^5.0"
    },
    "config": {
        "bin-dir": "vendor/bin",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\Bundle\\AdminBundle\\spec\\fixtures\\": "spec/fixtures",
            "FSi\\": "features/fixtures/project/src",
            "spec\\FSi\\Bundle\\AdminBundle\\": "spec/FSi/Bundle/AdminBundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev",
            "3.1": "3.1-dev",
            "3.0": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.1": "1.1-dev"
        }
    }
}
